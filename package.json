		"@types/styled-components": "^5.1.26",
		"autoprefixer": "10.4.14",
		"eslint-config-next": "13.4.12",
		"next": "13.4.12",
		"next-pwa": "^5.6.0",
		"next-themes": "^0.2.1",
		"postcss": "8.4.27",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"styled-components": "^6.0.7",
		"tailwindcss": "3.3.3",
		"typescript": "5.1.6"
	},
	"devDependencies": {
		"@storybook/addon-controls": "^7.2.1",
